{
	"name": "Twitica Desktop",
	"version": "2.0.6.1",
	"description": "A Twitter client focusing on timeline view.",
	"options_page": "twplus/options.html",
	"app": {
		"launch": {
			"local_path": "index.html",
			"container": "panel",
			"height": 650,
			"width": 500
		}
	},
	"permissions": [
		"https://api.twitter.com/",
		"https://userstream.twitter.com/",
		"http://stream.twitter.com/",
		"http://maps.google.com/",
		"https://twitpic.com/",
		"http://api.twitpic.com/",
		"tabs",
		"notifications",
		"geolocation",
		"unlimitedStorage"
	],
	"icons": {
		"16": "logo16.png",
		"48": "logo48.png",
		"128": "logo.png"
	},
	"minimum_chrome_version":  "9.0"
}
